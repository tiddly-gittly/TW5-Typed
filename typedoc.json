{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/*"],
  "exclude": ["./src/plugins"],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "compilerOptions": {
    "strictNullChecks": false,
    "skipLibCheck": true,
    "rootDir": ".",
  },
  "readme": "none",
  "entryDocument": "API.md",
  "plugin": ["typedoc-plugin-markdown"]
}