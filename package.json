{
  "name": "tw5-typed",
  "version": "0.7.0",
  "scripts": {
    "check": "tsc --noEmit && eslint src/**/*.ts",
    "docs": "docs-ts",
    "docs:build": "docusaurus build",
    "docs:dev": "docusaurus start",
    "docs:generate": "npm run docs:generate:api && npm run docs:generate:copy",
    "docs:generate:api": "rimraf docs/api && typedoc --options typedoc.json",
    "docs:generate:copy": "zx scripts/copy-readme.mjs",
    "update": "npm-check-updates -u && dprint config update",
    "prepublishOnly": "tsc --noEmit"
  },
  "description": "Types for tiddlywiki",
  "license": "MIT",
  "url": "https://github.com/tiddly-gittly/tw5-typed",
  "homepage": "https://github.com/tiddly-gittly/tw5-typed",
  "bugs": {
    "url": "https://github.com/tiddly-gittly/tw5-typed/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiddly-gittly/tw5-typed.git"
  },
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.d.ts",
      "require": "./src/index.d.ts",
      "types": "./src/index.d.ts"
    }
  },
  "files": [
    "src/"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ],
    "*.{js,jsx,mjs,mjsx,cjs,cjsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ]
  },
  "dependencies": {
    "@types/codemirror": "^5.60.17",
    "@types/echarts": "^5.0.0",
    "@types/node": "^24.10.1"
  },
  "devDependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@docusaurus/types": "^3.9.2",
    "@mdx-js/react": "^3.1.1",
    "@modern-js/eslint-config": "latest",
    "@modern-js/tsconfig": "latest",
    "docs-ts": "^0.8.0",
    "dprint": "^0.50.2",
    "eslint-config-tidgi": "^2.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "npm-check-updates": "^19.1.2",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.0",
    "rimraf": "^6.1.0",
    "tiddlywiki": "^5.3.8",
    "type-fest": "^5.2.0",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "zx": "^8.8.5"
  }
}
