{
  "description": "Types for tiddlywiki",
  "license": "MIT",
  "name": "tw5-typed",
  "version": "0.4.10",
  "url": "https://github.com/tiddly-gittly/tw5-typed",
  "homepage": "https://github.com/tiddly-gittly/tw5-typed",
  "bugs": {
    "url": "https://github.com/tiddly-gittly/tw5-typed/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiddly-gittly/tw5-typed.git"
  },
  "types": "src/index.d.ts",
  "files": [
    "src/"
  ],
  "scripts": {
    "check": "tsc --noEmit && eslint src/**/*.ts",
    "prepublishOnly": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ],
    "*.{js,jsx,mjs,mjsx,cjs,cjsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ]
  },
  "devDependencies": {
    "@modern-js/eslint-config": "latest",
    "@modern-js/tsconfig": "latest",
    "dprint": "^0.45.0",
    "eslint-config-tidgi": "^1.1.5",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "rimraf": "^3.0.2",
    "tiddlywiki": "^5.2.5",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/codemirror": "^5.60.6",
    "@types/echarts": "^4.9.16",
    "@types/node": "^18.11.9"
  }
}
